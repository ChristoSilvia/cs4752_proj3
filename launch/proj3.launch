<launch>
	<param name="num_blocks" value="10" type="int"/>
	<param name="configuration" value="stacked_descending" />
        <!-- environment can be "symbolic", "simulator", "robot" -->
	<param name="environment" value="simulator" />
	<param name="limb" value="left" />

    <node name="controller" pkg="cs4752_proj3" type="controller.py" output="screen"></node>
    <node name="robot_interface" pkg="cs4752_proj3" type="robot_interface.py" output="screen"></node>
	<node name="joint_action_server" pkg="cs4752_proj3" type="joint_action_server.py" output="screen"></node>
</launch>